/* 
* This program shows that array index must be of type int. Errors will be
* shown when running type checking.
*/

void minloc(int a[], int low, int high) {
  int i; int x; int k;
  k = low;
  x = a[low];
  i = low + 1;
  k = a[minloc()];
  x = a[a[a[a[a[1]]]]];
  
  while (i < high) {
    if (a[i] < x) {
       x = a[i];
       k = i; 
    }
    i = i + 1;
  } 
  return;
}